
<div class="container">
      <%= raw @page.content_for(:body) %>
       <%= raw @page.content_for(:side_body) %>
       <div class="box">
            <div class="col-lg-12">
                <hr>
                <h2 class="intro-text text-center"><%=@page.title%>
                </h2>
                <hr>
            </div>
            <section id='gallery'>
              <%= render :partial => "image", :collection => @page.images.paginate(:page => params[:page], :per_page => 16) %>
            </section>
            <%= will_paginate @page.images.paginate(:page => params[:page], :per_page => 16)%>
          </div>
</div>

 <script type="text/javascript">
  
    $(document).ready(function() {
      $(".fancybox").fancybox({
      helpers : {
        title : {
          type : 'float'
        }
      }
      });
    });
</script>